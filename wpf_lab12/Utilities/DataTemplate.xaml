<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:wpf_lab12.ViewModel"
                    xmlns:view="clr-namespace:wpf_lab12.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AdminVM}">
        <view:Admin/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:GoodsVM}">
        <view:Goods/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OrdersVM}">
        <view:Orders/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CustomersVM}">
        <view:Customers/>
    </DataTemplate>
</ResourceDictionary>